{
    "Bundle-SymbolicName": "devnet_query_builder",
    "Bundle-Version": "2.0.0",
    "Bundle-Name": "${bundleName}",
    "Bundle-Description": "${bundleDescription}",
    "Bundle-Vendor": "con terra GmbH",
    "Product-Name": "map.apps Developer Network",
    "Config-Bundle-Location": "/config",
    "CSS-Themes-Extension": [{
            "name": "*",
            "files": ["./styles/styles.css"]
        }],
    "Components": [{
            "name": "QueryTools",
            "impl": "ct.tools.Tool",
            "provides": ["ct.tools.Tool"],
            "propertiesConstructor": true,
            "componentFactory": true,
            "properties": {
                "id": "querytool",
                "title": "Query a Store",
                "toolRole": "toolset",
                "iconClass": "icon-info",
                "clickHandler": "onFilterStoreClicked",
                "customquery": {},
                "storeId": "*"
            },
            "references": [{
                    "name": "handlerScope",
                    "providing": "filter_query.FilterQuery"
                }, {
                    "name": "store",
                    "providing": "ct.api.Store",
                    "filter": "(id={storeId})"
                }]
        }, {
            "name": "FilterQuery",
            "provides": ["filter_query.FilterQuery"],
            "references": [{
                    "name": "_eventService",
                    "providing": "ct.framework.api.EventService"
                }]
        }/*, {
            "name": "UserToolCreator",
            "provides": [
                "dijit.Widget",
                "analysis_distribution.TabWidgetFactory"
            ],
            "properties": {},
            "instanceFactory": true,
            "immediate": true,
            "references": [
                {
                    "name": "store",
                    "providing": "ct.api.Store",
                    "filter": "(id={storeId})"
                },
                {
                    "name": "_mapState",
                    "providing": "ct.mapping.map.MapState"
                },
                {
                    "name": "_tool",
                    "providing": "ct.tools.Tool",
                    "filter": "(id=analysisDistributionTool)"
                }
            ]
        },
        {
            "name": "${toolName}",
            "impl": "ct/tools/Tool",
            "provides": ["ct.tools.Tool"],
            "propertiesConstructor": true,
            "properties": {
                "id": "userToolCreatorTool",
                "toolRole": "toolset",
                "title": "UserToolCreator",
                "tooltip": "",
                "togglable": false,
                "iconClass": "icon-chart-bar"
            }
        }*/]
}
